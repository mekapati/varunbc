{
  "name": "seeder",
  "version": "1.0.0",
  "description": " ",
  "scripts": {
    "start": "webpack serve --config=config/webpack-app.dev.js --open --hot",
    "build": "export NODE_OPTIONS='--max-old-space-size=4096' && webpack --config=config/webpack-app.prod.js",
    "test": "node --expose-gc --max-old-space-size=4096 ./node_modules/.bin/jest --config jest.config.js --coverage --runInBand --logHeapUsage --detectOpenHandles --forceExit",
    "generate:snapshot": "node --expose-gc --max-old-space-size=4096 ./node_modules/.bin/jest -u --config jest.config.js --runInBand --logHeapUsage --detectOpenHandles --forceExit",
    "test:coverage": "node --expose-gc --max-old-space-size=4096 ./node_modules/.bin/jest  --config jest.config.js --runInBand --logHeapUsage --detectOpenHandles --forceExit --coverage --testResultsProcessor ./node_modules/jest-sonar-reporter/index.js",
    "sonar": "sonar-scanner",
    "format": "prettier --write **/*.{ts,tsx,js,jsx}",
    "lint": "tsc --noEmit && eslint **/*.{ts,tsx,js,jsx} --fix",
    "storybook": "start-storybook -p 6006 -c .storybook watch-css -s ./src",
    "build-storybook": "build-storybook -s ./src",
    "lint:staged": "eslint --fix"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/lab": "5.0.0-alpha.86",
    "@mui/material": "5.8.0",
    "@mui/styles": "5.8.0",
    "@mui/x-data-grid": "5.8.0",
    "@types/react-router-dom": "5.3.3",
    "auth0-js": "^9.19.0",
    "axios": "0.27.2",
    "classnames": "2.3.1",
    "mini-css-extract-plugin": "2.6.0",
    "moment": "^2.29.3",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-query": "3.39.0",
    "react-router-dom": "6.3.0",
    "react-router-hash-link": "2.4.3",
    "react-router-navigation-prompt": "1.9.6",
    "react-toastify": "^9.0.5",
    "subscriptions-transport-ws": "0.9.18",
    "webpack-manifest-plugin": "5.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.0",
    "@babel/preset-env": "7.18.0",
    "@babel/preset-react": "7.17.12",
    "@faker-js/faker": "^7.3.0",
    "@mswjs/data": "^0.10.1",
    "@storybook/addon-actions": "6.5.3",
    "@storybook/addon-essentials": "6.5.3",
    "@storybook/addon-knobs": "6.1.21",
    "@storybook/addon-links": "6.5.3",
    "@storybook/react": "6.5.3",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.3",
    "@testing-library/react-hooks": "8.0.0",
    "@types/auth0-js": "^9.14.6",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.35",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@types/react-infinite-scroller": "1.2.3",
    "@types/webpack-env": "1.17.0",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "@webpack-cli/serve": "1.6.1",
    "babel-loader": "8.2.5",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "10.0.0",
    "css-loader": "6.7.1",
    "dagre": "0.8.5",
    "eslint": "8.15.0",
    "eslint-config-zemoso": "1.0.3",
    "eslint-plugin-react": "7.30.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "jest-diff": "28.1.0",
    "jest-environment-jsdom": "28.1.1",
    "jest-fetch-mock": "3.0.3",
    "jest-raw-loader": "1.0.1",
    "jest-sonar-reporter": "2.0.0",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "12.4.1",
    "ml-matrix": "6.10.0",
    "msw": "0.39.2",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss": "8.4.14",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "raw-loader": "4.0.2",
    "react-test-renderer": "18.1.0",
    "resize-observer-polyfill": "1.5.1",
    "sonar-scanner": "3.1.0",
    "storybook-addon-designs": "6.2.1",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-jest": "28.0.2",
    "ts-loader": "9.3.0",
    "ts-node": "10.7.0",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "tslint-sonarts": "1.9.0",
    "typescript": "4.6.4",
    "url-loader": "4.1.1",
    "webpack": "5.72.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "4.9.0",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,css}": [
      "npm run lint:staged",
      "npm run format"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
